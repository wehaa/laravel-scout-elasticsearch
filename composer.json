{
    "name": "wehaa/laravel-scout-elasticsearch",
    "description": "Elasticsearch Driver for Laravel Scout",
    "keywords": ["laravel", "scout", "elasticsearch"],
    "require": {
        "php": ">=7.1",
        "laravel/scout": "^8.0|^9.0",
        "elasticsearch/elasticsearch": "^6.0"
    },
    "require-dev": {
        "mockery/mockery": "^1.2",
        "phpunit/phpunit": "~6.0"
    },
    "autoload": {
        "psr-4": {
            "Wehaa\\ScoutElasticsearch\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Wehaa\\ScoutElasticsearch\\Test\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Wehaa\\ScoutElasticsearch\\ScoutElasticsearchServiceProvider"
            ]
        }
    }
}
